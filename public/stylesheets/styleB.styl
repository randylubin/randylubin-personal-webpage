body
  background: steelblue
  color: beige
  font-family: 'Source Sans Pro', sans-serif

p
  font-size: 1.2em
  font-family: 'Source Sans Pro', sans-serif

a:link, a:visited
  color: #FFD8A3

a:hover
  color: #ffca82
  text-decoration: none

h1
  font-size: 3em
  color: beige

h2
  font-size: 2.2em

.accentText
  color: #ffb958

.topBit
  font-family: 'Domine', serif
  margin-top: 100px
  margin-bottom: 15px

.navS p
  font-size: 1.35em
  font-family: 'Domine', serif
  margin-top: 1px
  margin-bottom: 12px

.accentText a:link, .accentText a:visited
  color: #ffb958

.accentText a:hover
  color: #A66C1D
  text-decoration: none 

.pic
  width: 65px
  height: 65px
  background-color: grey
  -webkit-border-radius: 100px
  border-radius: 100px
  -webkit-box-shadow: 0px 0px 0px 2px #F5F5DC
  box-shadow: 0px 0px 0px 2px #F5F5DC

.left
  margin-right: 20px

.slightIndent
  margin-left: 10px
  margin-bottom: 10px

.projectHeading
  margin-top: -5px

