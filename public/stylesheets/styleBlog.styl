body
  background-color: #174a75
  color: beige
  font-family: 'Source Sans Pro', sans-serif


p
  font-size: 1.2em
  font-family: 'Source Sans Pro', sans-serif

a:link, a:visited
  color: #FFD8A3

a:hover
  color: #ffca82
  text-decoration: none

h1
  font-size: 3em
  color: beige

h2
  font-size: 2em

.accentText, .accentText h1
  color: #ffcd8c

.topBit
  font-family: 'Domine', serif
  margin-top: 100px
  margin-bottom: 15px

.navS p
  font-size: 1.35em
  font-family: 'Domine', serif
  margin-top: 1px
  margin-bottom: 12px

.accentText a:link, .accentText a:visited
  color: #ffcd8c

.accentText a:hover
  color: #A66C1D
  text-decoration: none 

.pic
  width: 65px
  height: 65px
  background-color: grey
  margin-top: 25%;

  -webkit-border-radius: 100px
  border-radius: 100px
  -webkit-box-shadow: 0px 0px 0px 2px #F5F5DC
  box-shadow: 0px 0px 0px 2px #F5F5DC

.left
  margin-right: 20px
  width: 70px

.slightIndent
  margin-left: 10px
  margin-bottom: 10px

.projectHeading
  margin-top: -5px

.tabletPadding
  height: 18px